    #font = CPMono_v07 12*/
    scrollback_lines = 1000
    search_wrap = true
    fullscreen = true
    #icon_name = terminal
    geometry = 2560x1440

    # "system", "on" or "off"
    cursor_blink = off

    # "block", "underline" or "ibeam"
    cursor_shape = block

    # $BROWSER is used by default
    browser = palemoon
[colors]
# hard contrast: background = #263238
# soft contrast: background = #263238
background = #263238
cursor          = #c1cdd3
foreground = #aabbc3
foreground_bold = #aabbc3

# Black + DarkGrey
color0 = #263238
color8 = #415a63

# DarkRed + Red
color1 = #dd6880
color9 = #f1879d

# DarkGreen + Green
color2 = #83b879
color10 = #89cf7c

# DarkYellow + Yellow
color3 = #ddb76f
color11 = #f1ce8e

# DarkBlue + Blue
color4 = #7aa5e6
color12 = #9dc1f6

# DarkMagenta + Magenta
color5 = #ad88e2
color13 = #c8aaf3
# DarkCyan + Cyan
color6 = #63c9d6
color14 = #80e0ec

# LightGrey + White
color7 = #aabbc3
color15 = #c1cdd3


