    font=Source Code Pro Medium 13
    #font=Envy Code R VS 14
    #font=Menlo\ for\ Powerline\ 14
    #font = CPMono_v07 12*/
    scrollback_lines = 1000
    search_wrap = true
    fullscreen = true
    #icon_name = terminal
    geometry = 2560x1440

    # "system", "on" or "off"
    cursor_blink = off

    # "block", "underline" or "ibeam"
    cursor_shape = block

    # $BROWSER is used by default
    browser = palemoon
[colors]
# hard contrast: background = #3a3a3a
# soft contrast: background = #3a3a3a
background = #3a3a3a
cursor          = #d0d0d0
foreground = #d0d0d0
foreground_bold = #d0d0d0

# Black + DarkGrey
color0 = #3a3a3a
color8 = #626262

# DarkRed + Red
color1 = #d68787
color9 = #d75f87

# DarkGreen + Green
color2 = #5f865f
color10 = #87af87

# DarkYellow + Yellow
color3 = #d8af5f
color11 = #ffd787

# DarkBlue + Blue
color4 = #85add4
color12 = #add4fb

# DarkMagenta + Magenta
color5 = #d7afaf
color13 = #ffafaf
# DarkCyan + Cyan
color6 = #87afaf
color14 = #87d7d7

# LightGrey + White
color7 = #d0d0d0
color15 = #e4e4e4


