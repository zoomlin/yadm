    font = Oxygen Mono Regular 13
    font = Consolas for Powerline Regular 13
    font = PT Mono Regular 12
    font = M+1mn Regular 13
    font=CPMono_v07 Plain 12
    font=Inconsolata Regular 14
    font=Roboto Mono Regular 13
    font=Ubuntu Mono Regular 14
    font=Monaco Regular 12
    font=Hack 13
