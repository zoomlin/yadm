[options]
    resize_grip = false
    scroll_on_output = false
    scroll_on_keystroke = true
    audible_bell = true
    visible_bell = false
    mouse_autohide = true
    allow_bold = false
    dynamic_title = true
    urgent_on_bell = true
    clickable_url = true
    #font=Meslo LG M DZ for Powerline 11
    #font=Terminus Re33 14
    font = Dejavu Sans Mono 14
    font = Inconsolatazi4 14
    #font=GohuFont 18
    #font=snap 12
    font=Dejavu Sans Mono 14
    #font=smoothansi 12
    font=Fira Mono Regular 14
    #font=oxygen Mono Regular 13
    font=monofur 16
    font=luculent 14
    font=Terminus re33 14
    font=ohsnap 12
    font = Hurmit 14
    font=iosevka 13
    font = PragmataPro 14
    font=Hermit Medium 13
